

#wrap {
  width: 100vw;
  height: 100vh;
  ---background-image: url(../images/bg.png);

}
/* 헤더 */
header {
  width: 100%;
  height: 45px;
  margin-bottom: 15px;
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: center;
  background-color: #081543;
}
/* 헤더 메뉴 */
header .menu {
  position: absolute;
  left: 0;
  width: 612px;
  height: 45px;
}
header .menu ul {
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
header .menu ul li a {
  font-size: 16px;
  color: #d9d9d9;
}
header .menu ul li:nth-child(1) {
  margin-left: 39px;
  margin-right: 90px;
}
header .menu ul li:nth-child(2) {
  margin-right: 90px;
}
header .menu ul li:nth-child(3) {
  margin-right: 90px;
}
/* 헤더 로고 */
header .logo {
  position: absolute;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 768px;
  height: 45px;
  background-image: url(../images/top_logo_bg.png);
}
/* 헤더 유저정보 */
header .user {
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  right: 0;
  width: 612px;
  height: 45px;
}
header .user div {
  color: #d9d9d9;
  font-size: 16px;
}
header .user div:nth-child(2) {
  margin-right: 14px;
}
header .user div:nth-child(3) {
  margin-right: 10px;
  cursor: pointer;
}

/* 검색창 */
.search {
  height: 100%;
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 477px;
  height: 49px;
  left: 9px;
  background-color: #4668d7;
  border-radius: 30px;
  border: 2px solid #3652ac;
  margin-top: 15px;
}
.search img:nth-child(1) {
  z-index: 2;
  margin-left: 30px;
}
.search .search_icon {
  position: absolute;
  z-index: 3;
  right: 20px;
}
.search img {
  cursor: pointer;
}
.search input {
  position: absolute;
  width: 265px;
  height: 30px;
  z-index: 1;
  left: 95px;
  border-radius: 20px;
  border: 0;
  background-color: #3652ac;
  font-size: 16px;
  color: #d9d9d9;
  padding-left: 60px;
}
.search input::placeholder {
  color: #d9d9d9;
}
/* 좌표 검색 */
.location {
  width: 720px;
  height: 40px;
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  bottom: 8px;
  right: 79px;
}
.location div {
  width: 185px;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-evenly;
  background-color: #edf2fc;
  font-size: 10pt;
  color: #191a1f;
  border: 2px solid #bebebd;
  border-radius: 15px;
}
.location div input {
  width: 110px;
  height: 25px;
  border: 1;
  border-color: #d9d9d9;
}
.location div:nth-child(3) {
  width: 317px;
}
/* section 메뉴 */
.section-men nav ul li a {
  color: black;
}
.section-menu {
  position: absolute;
  right: 79px;
  margin-top: 24px;
}
.section-menu nav {
  display: flex;
}
.section-menu nav ul .menu {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border-radius: 40px;
  border: 2px solid #bebebd;
  width: 159px;
  height: 40px;
  background-color: #edf2fc;
}
.section-menu nav ul .menu img {

margin-left:5px;
}
.section-menu nav ul .menu:hover {
  border: 2px solid #3652ac;
  cursor: pointer;
}
.section-menu nav ul .menu {
  margin-right: 10px;
}
.section-menu nav ul .menu4 {
  margin: 0;
}
.sub_menu_wrap1 {
  width: 159px;
  height: 90px;
  position: absolute;
  overflow: hidden;
  display: none;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  top: 49px;
  left: 0px;
  gap: 140px;
  transition: height 0.1s;
  z-index: 100000;
  border-radius: 20px;
  border: 2px solid #3652ac;
  background-color: #edf2fc;
}
.sub_menu_wrap1 .inner {
  width: 100%;
  padding-left: 50px;
  display: flex;
}
.sub_menu_wrap1 .inner ul li {
  padding: 3px 0;
}
.sub_menu_wrap2 .inner ul li {
  padding: 3px 0;
}
.sub_menu_wrap2 .inner {
  width: 100%;
  padding-left: 80px;
}
.sub_menu_wrap2 {
  width: 159px;
  height: 90px;
  position: absolute;
  overflow: hidden;
  display: none;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  top: 49px;
  left: 185px;
  gap: 140px;
  transition: height 0.1s;
  z-index: 100000;
  border-radius: 20px;
  border: 2px solid #2d4ba9;
  background-color: #edf2fc;
}
.subMenu {
  display: flex;
}
.subMenu img {
  margin-right: 10px;
}
.subMenu img:hover {
  cursor: pointer;
}
/* section 사이드 */
.section-sidebar {
  position: absolute;
  right: 5px;
  margin-top: 24px;
}
.section-sidebar .bar {
  width: 40px;
  height: 40px;
  margin-bottom: 2px;
  background: #edf2fc;
  border: 2px solid #bebebd;
  border-radius: 5px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  font-size: 10px;
  color: #191a1f;
}

.section-sidebar .bar:hover {
  border-color: #2d4ba9;
  cursor: pointer;
}

.section-sidebar .bar01 {
  width: 40px;
  height: 80px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.section-sidebar .bar-sub {
  position: absolute;
  display: none;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width: 140px;
  height: 40px;
  right: 50px;
  background-color: #edf2fc;
  border: 2px solid #2d4ba9;
  border-radius: 15px;
}
.section-sidebar .sub02 {
  width: 100px;
}
.section-sidebar .sub03 {
  right: 75px;
  bottom: 125px;
}
.section-sidebar .bar:hover .bar-sub {
  display: flex;
}

.section-sidebar .bar-sub div {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.section-sidebar .bar02 {
  width: 40px;
  height: 155px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.section-sidebar .bar03 {
  width: 40px;
  height: 140px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.section-sidebar .bar-0 {
  margin: 0;
  position: fixed;
  right: 5px;
  bottom: 5px;
}

.section-sidebar .bar .bar-menu {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
